{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env"],
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^dev"]
    },
    "dev:citi": {
      "outputs": []
    },
    "dev:jp": {
      "outputs": []
    },
    "dev:goldman": {
      "outputs": []
    },
    "dev:barclays": {
      "outputs": []
    },
    "dev:bny": {
      "outputs": []
    },
    "generateContracts": {
      "cache": false
    },
    "deployContracts": {
      "cache": false,
      "interactive": true
    }
  }
}
