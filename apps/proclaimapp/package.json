{
  "name": "proclaimapp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev:citi": "env-cmd -f .env.citi next dev -p 3001",
    "dev:jp": "env-cmd -f .env.jp next dev -p 3002",
    "dev:goldman:": "env-cmd -f .env.goldman next dev -p 3003",
    "dev:barclays": "env-cmd -f .env.barclays next dev -p 3004",
    "dev:bny": "env-cmd -f .env.bny next dev -p 3005",
    "dev": "concurrently \"npm run dev:citi\" \"npm run dev:jp\" \"npm run dev:goldman:\" \"npm run dev:barclays\" \"npm run dev:bny\"",
    "studio:citi": "env-cmd -f .env.citi prisma studio -p 3011",
    "studio:jp": "env-cmd -f .env.jp prisma studio -p 3012",
    "studio:goldman:": "env-cmd -f .env.goldman prisma studio -p 3013",
    "studio:barclays": "env-cmd -f .env.barclays prisma studio -p 3014",
    "studio:bny": "env-cmd -f .env.bny prisma studio -p 3015",
    "studio": "concurrently \"npm run studio:citi\" \"npm run studio:jp\" \"npm run studio:goldman\" \"npm run studio:barclays\" \"npm run studio:bny\"",
    "db:push:citi": "env-cmd -f .env.citi prisma db push",
    "db:push:jp": "env-cmd -f .env.jp prisma db push",
    "db:push:goldman": "env-cmd -f .env.goldman prisma db push",
    "db:push:barclays": "env-cmd -f .env.barclays prisma db push",
    "db:push:bny": "env-cmd -f .env.bny prisma db push",
    "db:push": "npm run db:push:citi && npm run db:push:jp && npm run db:push:goldman && npm run db:push:barclays && npm run db:push:bny",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.4.0",
    "@prisma/client": "^5.10.2",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tanstack/react-query": "^5.25.0",
    "@thirdweb-dev/contracts": "^3.8.0",
    "@trpc/client": "next",
    "@trpc/next": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "proclaim": "0.0.1",
    "crypto": "^1.0.1",
    "env-cmd": "^10.1.0",
    "ethers": "^5.7.2",
    "next": "^14.1.3",
    "next-auth": "^4.24.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "thirdweb": "^5.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/eccrypto": "^1.1.6",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.3",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2"
